SQL: INSERT INTO `toutiao_news` (`title`,`small_title`,`thumb`,`title_font_color`,`catid`,`copyfrom`,`description`,`keywords`,`create_time`,`username`) VALUES ('中西部14所：双一流下的兴奋、失落与挑战。','中西部14所：双一流下的兴奋、失落与挑战。','/upload/2017/10/07/59d85fdf14bd2.jpg','#5674ed','8','2','希望与期待 兴奋与挑战','希望与期待 兴奋与挑战','1507352587','caifeng') [ RunTime:0.0007s ]
