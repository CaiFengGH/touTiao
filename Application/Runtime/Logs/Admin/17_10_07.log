SQL: INSERT INTO `toutiao_news` (`title`,`small_title`,`thumb`,`title_font_color`,`catid`,`copyfrom`,`description`,`keywords`,`create_time`,`username`) VALUES ('如何看待视频交友下的互联网风口','如何看待视频交友下的互联网风口','/upload/2017/10/07/59d85a216e17f.png','#ed568b','9','1','就视频交友，产品应该修炼哪些能力','视频交友我们可能会遇到的面试问题','1507351133','caifeng') [ RunTime:0.0010s ]
