SQL: INSERT INTO `toutiao_news` (`title`,`small_title`,`thumb`,`title_font_color`,`catid`,`copyfrom`,`description`,`keywords`,`create_time`,`username`) VALUES ('我是中国人！史上最大“归国潮”来袭','我是中国人！史上最大“归国潮”来袭','/upload/2017/10/07/59d85f86e1f62.jpg','#5674ed','8','2','“归国潮”火了','“归国潮”火了','1507352494','caifeng') [ RunTime:0.0007s ]
