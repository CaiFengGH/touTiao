SQL: INSERT INTO `toutiao_news` (`title`,`small_title`,`thumb`,`title_font_color`,`catid`,`copyfrom`,`description`,`keywords`,`create_time`,`username`) VALUES ('看这家医院，如何成功联接了病人的“心房”','看这家医院，如何成功联接了病人的“心房”','/upload/2017/10/07/59d85965f255e.jpg','#5674ed','6','5','提高就诊率、改善医疗质量、提升治疗效率','提高就诊率、改善医疗质量、提升治疗效率','1507350955','caifeng') [ RunTime:0.0005s ]
